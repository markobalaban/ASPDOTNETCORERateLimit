{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },

  "RateLimiting": {
    "PerSecond": 5,
    "PerMinute": 0,
    "PerHour": 0,
    "PerDay": 0,
    "PerWeek": 0,
    "EnableIpRateLimiting": true,
    "EnableClientRateLimiting": true,
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429
  }
}
