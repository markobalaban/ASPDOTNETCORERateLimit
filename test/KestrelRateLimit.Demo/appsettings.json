{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },

  "RateLimiting": {
    "PerSecond": 3,
    "PerMinute": 6,
    "PerHour": 10,
    "PerDay": 0,
    "PerWeek": 0,
    "StoreOptionsInCache":  true,
    "StackBlockedRequests": false,
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "EnableIpRateLimiting": true,
    "IpWhitelist": [ "8.8.8.8", "192.168.0.0/24" ],
    "IpRules": [
      {
        "Value": "::1/10",
        "PerSecond": 2,
        "PerMinute": 60,
        "PerHour": 0,
        "PerDay": 0,
        "PerWeek": 0
      },
      {
        "Value": "192.168.1.2",
        "PerSecond": 2,
        "PerMinute": 30,
        "PerHour": 0,
        "PerDay": 0,
        "PerWeek": 0
      }
    ],
    "EnableEndpointRateLimiting": true,
    "EndpointWhitelist": [ "post:/api/items", "get:/api/items" ],
    "EndpointRules": [
      {
        "Value": "get:/api/values",
        "PerSecond": 1,
        "PerMinute": 10,
        "PerHour": 0,
        "PerDay": 0,
        "PerWeek": 0
      },
      {
        "Value": "post:/api/values",
        "PerSecond": 2,
        "PerMinute": 30,
        "PerHour": 0,
        "PerDay": 0,
        "PerWeek": 0
      }
    ],
    "EnableClientRateLimiting": true,
    "ClientWhitelist": [ "12312sdsd2", "xx222xxx" ],
    "ClientRules": [
      {
        "Value": "yy312sdsd2",
        "PerSecond": 3,
        "PerMinute": 50,
        "PerHour": 0,
        "PerDay": 0,
        "PerWeek": 0
      },
      {
        "Value": "345345gbgg",
        "PerSecond": 2,
        "PerMinute": 30,
        "PerHour": 0,
        "PerDay": 0,
        "PerWeek": 0
      }
    ]
  }
}
