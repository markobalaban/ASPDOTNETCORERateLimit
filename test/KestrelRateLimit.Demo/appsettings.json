{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },

  "RateLimiting": {
    "PerSecond": 2,
    "PerMinute": 6,
    "PerHour": 10,
    "PerDay": 0,
    "PerWeek": 0,
    "StoreOptionsInCache": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "BypassHeader": "X-Bypass",
    "HttpStatusCode": 429,
    "IpWhitelist": [ "8.8.8.8", "192.168.0.0/24" ],
    "IpRules": [
      {
        "Value": "::1/10",
        "PerSecond": 2,
        "PerMinute": 60,
        "PerHour": 0,
        "PerDay": 0,
        "PerWeek": 0
      },
      {
        "Value": "84.247.85.226",
        "PerSecond": 1,
        "PerMinute": 6,
        "PerHour": 0,
        "PerDay": 0,
        "PerWeek": 0
      }
    ],
    "EnableEndpointRateLimiting": true,
    "EndpointWhitelist": [ "delete:/api/values", "get:/api/items" ],
    "EndpointRules": [
      {
        "Value": "get:/api/values",
        "PerSecond": 1,
        "PerMinute": 10,
        "PerHour": 0,
        "PerDay": 0,
        "PerWeek": 0
      },
      {
        "Value": "post:/api/values",
        "PerSecond": 2,
        "PerMinute": 30,
        "PerHour": 0,
        "PerDay": 0,
        "PerWeek": 0
      }
    ],
    "ClientWhitelist": [ "bypass-key-1", "bypass-key-2" ]
  }
}
